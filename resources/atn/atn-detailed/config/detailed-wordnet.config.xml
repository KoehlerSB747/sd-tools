<?xml version="1.0" encoding="utf-8" ?>

<parse>

  <resources>
    <resource name="wn-dict">
      <jclass>org.sd.atnexec.WordNetDictionaryResource</jclass>
    </resource>
  </resources>

  <compoundParser>
    <id>VerbSequence</id>
    <parser file="${atnDir}/config/vs-config.xml" />
  </compoundParser>

  <compoundParser>
    <id>NounPhrase</id>
    <parser file="${atnDir}/config/np-config.xml" />
  </compoundParser>

  <compoundParser>
    <id>PrepPhrase</id>
    <parser file="${atnDir}/config/pp-config.xml" />
  </compoundParser>

  <compoundParser>
    <id>WordNet</id>
    <parser file="${atnDir}/config/wordnet-config.xml" />
  </compoundParser>

</parse>
